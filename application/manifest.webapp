{
  "name": "Audio Visualization",
  "description": "A Simple listening app",
  "type": "privileged",
  "role": "input",
  "launch_path": "/settings.html",
  "icons": {
    "16": "/icons/icon16x16.png",
    "48": "/icons/icon48x48.png",
    "60": "/icons/icon60x60.png",
    "128": "/icons/icon128x128.png"
  },
  "developer": {
    "name": "MikaSez",
    "url": "https://github.com/MikaSez/"
  },
  "permissions": {
    "input": {
      "description": "Send key events and characters to user focused input fields."
    },
   "audio-capture": {
     "description": "Required to capture audio via getUserMedia"
   },
    "speech-recognition" : {
      "description" : "Speech recognition"
    }
  },
  "inputs": {
    "audioVisual": {
      "launch_path": "/index.html",
      "name": "AudioVisual",
      "description": "Audio layout",
      "types": ["text", "url", "number"]
    }
  },
  "default_locale": "fr-FR"
}